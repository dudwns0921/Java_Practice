package chapter3_9;

public abstract class Shape {
  public double height, width;

  public Shape(double h, double w) {
    height = h;
    width = w;
  }

  public double getHeight() {
    return height;
  }

  public void setHeight(double height) {
    this.height = height;
  }

  public double getWidth() {
    return width;
  }

  public void setWidth(double width) {
    this.width = width;
  }

}
